	<queryString>
		<![CDATA[SELECT *
FROM
(
    (
        SELECT DATE_FORMAT(getPurcDate(p.purcID),'%m/%d/%y %h:%i %p') AS date,CONCAT('(IN) PO ',p.purcID) as subject, p.prodQuantity as qty, p.prodCost as cost, p.stock as stock, p.stock+p.prodQuantity as after, getPurcAssign(purcID) as account, DATE_FORMAT(NOW(),'%m/%d/%y %h:%i %p') AS NOW, prodID as prodID, getProdName(prodID) as prodName FROM po_product_list p WHERE p.prodID=$P{prodID} AND DATEDIFF(getPurcDate(p.purcID),$P{from})>=0 AND DATEDIFF(getPurcDate(p.purcID),$P{to})<=0
   )
    UNION
    (SELECT DATE_FORMAT(getSaleDate(s.saleID),'%m/%d/%y %h:%i %p') AS date, CONCAT('(OUT) SO ',s.saleID) as subject,s.prodQty as qty, s.netPrice as cost, s.stock as stock, s.stock+s.prodQty as after, getAccName(getSaleAssign(s.saleID)) as account, DATE_FORMAT(NOW(),'%m/%d/%y %h:%i %p') AS NOW, prodID as prodID, getProdName(prodID) as prodName FROM so_product_list s WHERE prodID=$P{prodID} AND DATEDIFF(getSaleDate(saleID),$P{from})>=0 AND DATEDIFF(getSaleDate(saleID),$P{to})<=0 )
) TEMP
ORDER BY date ASC;]]>
	</queryString>
